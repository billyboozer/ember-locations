<div class="col-md-12">
  {{#view App.MapView contentBinding='this'}}
    <div id="map"></div>
  {{/view}}
</div>
<div class="col-md-3">
  <h1>Location</h1>
  <ul>
    {{#each location in controller}}
      {{#link-to 'location' location tagName="li"}}
        {{location.title}}
      {{/link-to}}
    {{/each}}
  </ul>
</div>

<div class="col-md-3">
  {{outlet}}
</div>
